  "name": "example-service",
  "version": "1.0.0",
  "description": "example node webservice",
  "main": "index.js",
  "scripts": {
    "build:local": "gulp build:local",
    "build:staging": "gulp build:staging",
    "build:develop": "gulp build:develop",
    "build:production": "gulp build:production",
    "start": "NODE_ENV=local nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "start:window": "NODE_ENV=local nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec src/index.ts",
    "local": "gulp build:local  && (tsc -w & nodemon dist/index.js local)",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "document": "gulp document-watch",
    "verify-lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint -c tslint.json -p tsconfig.json"
    }
  },
